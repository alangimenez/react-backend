<table id="destino" class="table">
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Descripcion</th>
        <th scope="col">Stock</th>
        <th scope="col">Precio</th>
        <th scope="col">Foto</th>
    </tr>
    <div></div>
    {{#each listaDeProductos}}
    <tr>
        <td>{{this.nombre}}</td>
        <td>{{this.descripcion}}</td>
        <td>{{this.stock}}</td>
        <td>{{this.precio}}</td>
        <td>{{this.foto}}</td>
    </tr>
    {{/each}}
</table>