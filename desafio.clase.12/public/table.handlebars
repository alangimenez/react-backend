<table id="destino" class="table">
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Autor</th>
        <th scope="col">Categoria</th>
        <th scope="col">Precio</th>
    </tr>
    <div></div>
    {{#each listaDeProductos}}
    <tr>
        <td>{{this.name}}</td>
        <td>{{this.autor}}</td>
        <td>{{this.categoria}}</td>
        <td>{{this.precio}}</td>
    </tr>
    {{/each}}
</table>