    <style>
        .spinner {
            display: none;
            width: 2em;
            height: 2em;
            border-top: 1em solid #6da9f5;
            border-right: 1em solid transparent;
            border-radius: 50%;
            margin: 1rem 1rem;
            animation: spinner 0.6s linear infinite;
        }

        .head {
            width: 1em;
            height: 1em;
            background-color: #6da9f5;
            border-radius: 50%;
            margin-left: 0.5em;
            margin-top: 0.5em;
        }

        @keyframes spinner {
            100% {
                transform: rotate(360deg)
            }
        }
    </style>
</head>

